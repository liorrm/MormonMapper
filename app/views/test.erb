<h1>List of Temples</h1>
<ol>
<% @temples.each do |temple| %>
  <li><%= temple.name %></li>
  <li class='json'><%= temple.to_json %></li>
    <ul>
      <li><%= temple.snippet %></li>
      <li><%= temple.address %></li>
      <li><%= temple.link %></li>
      <li><%= temple.latitude %>, <%= temple.longitude %></li>
    </ul>
<% end %>
</ol>